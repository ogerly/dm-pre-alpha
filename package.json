{
  "name": "dreammall-prototype",
  "private": true,
  "version": "dream-0.2-alpha",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ogerly/dm-pre-alpha.git"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "vite",
    "build": "vite build",
    "build:check": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "node run-eslint.js",
    "lint:fix": "eslint --fix src/**/*.{vue,js,jsx,ts,tsx}",
    "build:nolint": "SKIP_LINT=true vite build",
    "build:warn": "ESLINT_WARNINGS_ONLY=true vite build",
    "typecheck": "vue-tsc --noEmit",
    "deploy:local": "node scripts/deploy.js",
    "deploy": "npm run build && gh-pages -d dist",
    "docs:install": "cd docs && npm install",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview"
  },
  "dependencies": {
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.0.10",
    "@tsparticles/vue3": "3.0.1",
    "@vueuse/core": "12.8.2",
    "express": "4.21.2",
    "framer-motion": "12.4.10",
    "pinia": "^2.1.7",
    "socket.io": "4.8.1",
    "socket.io-client": "4.8.1",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@inspira-ui/plugins": "0.0.1",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-vue": "4.6.2",
    "@vue/tsconfig": "0.5.1",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.33.0",
    "gh-pages": "6.3.0",
    "postcss": "^8.4.31",
    "tailwind-merge": "3.0.2",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.2.2",
    "vite": "5.2.0",
    "vite-plugin-eslint": "1.8.1",
    "vue-eslint-parser": "9.4.3",
    "vue-tsc": "1.8.27"
  }
}
